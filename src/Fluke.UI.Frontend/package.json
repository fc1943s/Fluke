{
  "name": "fluke",
  "displayName": "Fluke",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/fc1943s/fluke.git"
  },
  "author": "fc1943s",
  "license": "MIT",
  "homepage": ".",
  "private": true,
  "scripts": {
    "build:fable": "dotnet tool restore && dotnet paket install && dotnet fable",
    "build:react": "cross-env CI= INLINE_RUNTIME_CHUNK=false EXTEND_ESLINT=true SKIP_PREFLIGHT_CHECK=true react-app-rewired build && rm -rf dist && mv build dist",
    "parcel-build:react": "parcel build static/index.html --detailed-report && cp -r static/manifest.json static/404.html static/sounds static/icons dist",
    "watch:frontend-build": "dotnet fable watch",
    "watch:frontend": "cross-env HTTPS=true EXTEND_ESLINT=true BROWSER=none FAST_REFRESH=true PORT=33922 SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "old-watch:frontend-parcel": "cross-env EXTEND_ESLINT=true parcel public/index.html --https --port 33922 --detailed-report",
    "watch": "concurrently \"yarn:watch:*\"",
    "enable-build-watch": "yarn cra-build-watch",
    "disable-build-watch": "yarn --check-files",
    "build": "yarn build:fable && yarn build:react",
    "gatsby-build": "cross-env NODE_ENV=production gatsby build",
    "gatsby-develop": "cross-env NODE_ENV=development gatsby develop",
    "ts2fable-react-icons": "ts2fable node_modules/react-icons/all.d.ts src/Bindings/Vendor/ReactIcons.fs -e react-icons",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --rootDir . --runInBand --testNamePattern=",
    "cypress-install": "cypress install --force",
    "cypress": "cypress open",
    "cypress-watch": "concurrently \"yarn:watch\" \"yarn:cypress\"",
    "cypress-run": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 cypress run --headed",
    "frontend-serve": "cross-env PORT=33922 serve dist",
    "test-watch": "yarn test --watchAll",
    "test-build-watch": "concurrently \"yarn:watch:frontend-build\" \"yarn:test-watch\"",
    "postinstall": "patch-package",

    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",

    "cordova-requirements": "cordova requirements",
    "emulate-android": "cordova emulate android",
    "build-android": "yarn build && rm -rf www && cp -r dist www && cordova build android --prod --release -- --keystore ~/.android/debug.keystore",
    "deploy-android": "adb install -d -r ./platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk",
    "run-android": "cordova run android --device"
  },
  "dependencies": {
    "@fontsource/roboto-condensed": "~4.4",

    "react": "0.0.0-experimental-28625c6f4",
    "react-dom": "0.0.0-experimental-28625c6f4",

    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",

    "recoil": "facebookexperimental/Recoil.git#nightly",
    "recoilize": "~2.0",

    "gun": "amark/gun.git#manhattan",

    "@chakra-ui/react": "~1.6",
    "@chakra-ui/system": "~1.6",
    "@chakra-ui/icons": "~1.0",
    "@chakra-ui/theme-tools": "~1.1",
    "@chakra-ui/media-query": "~1.0",
    "@emotion/react": "~11.4",
    "@emotion/styled": "~11.3",
    "framer-motion": "~4.1",
    "react-icons": "~4.2",

    "electron-window-state": "~5.0",
    "electron-devtools-installer": "~3.2",
    "devtron": "~1.4",

    "react-checkbox-tree": "~1.6",

    "react-markdown": "~6.0",
    "re-resizable": "~6.9",
    "date-fns": "~2.22",
    "jssha": "~3.2"
  },
  "devDependencies": {
    "@types/react": "~17.0",
    "@types/gun": "~0.9",

    "typescript": "~4.3",
    "ts-node": "~10.0",
    "@types/node" : "~15.9",
    "ts2fable" : "~0.8.0-build.615",

    "fable-loader": "~2.1",
    "fable-compiler": "~2.13",

    "electron": "~14.0.0-beta.2",
    "electron-webpack": "~2.8",
    "electron-builder": "~22.10",

    "@typescript-eslint/eslint-plugin": "~4.26",
    "@typescript-eslint/parser": "~4.26",
    "eslint": "~7.27",
    "eslint-plugin-react": "~7.24",
    "eslint-plugin-import": "~2.23",
    "eslint-plugin-jsx-a11y": "~6.4",
    "eslint-plugin-react-hooks": "~4.2",
    "eslint-plugin-jest-dom": "~3.9",

    "gatsby": "~3.6.2",
    "@chakra-ui/gatsby-plugin": "~2.0",
    "postcss": "~8.3",

    "jest": "~27.0",
