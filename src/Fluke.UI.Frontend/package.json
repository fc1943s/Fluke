{
  "name": "fluke",
  "displayName": "Fluke",
  "version": "0.0.1",
  "description": "Opinionated Information manager to tackle decision fatigue",
  "repository": {
    "type": "git",
    "url": "https://github.com/fc1943s/fluke.git"
  },
  "author": "fc1943s",
  "license": "MIT",
  "homepage": ".",
  "private": true,
  "scripts": {
    "build:fable": "dotnet tool restore && dotnet paket install && dotnet fable",
    "build:react": "rm -rf build && cross-env CI= INLINE_RUNTIME_CHUNK=false EXTEND_ESLINT=true SKIP_PREFLIGHT_CHECK=true react-app-rewired build && rm -rf cra_output && mv build cra_output",
    "parcel-build:react": "parcel build static/index.html --detailed-report && cp -r static/manifest.json static/404.html static/sounds static/icons parcel_output",
    "watch:frontend-build": "dotnet fable watch",
    "watch:frontend": "cross-env HTTPS=true EXTEND_ESLINT=true BROWSER=none FAST_REFRESH=true PORT=33922 SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
    "old-watch:frontend-parcel": "cross-env EXTEND_ESLINT=true parcel public/index.html --https --port 33922 --detailed-report",
    "watch": "concurrently \"yarn:watch:*\"",
    "enable-build-watch": "yarn cra-build-watch",
    "disable-build-watch": "yarn --check-files",
    "build": "yarn build:fable && yarn build:react",
    "gatsby-build": "cross-env NODE_ENV=production gatsby build",
    "gatsby-develop": "cross-env NODE_ENV=development gatsby develop",
    "ts2fable-react-icons": "ts2fable node_modules/react-icons/all.d.ts src/Bindings/Vendor/ReactIcons.fs -e react-icons",
    "test-parallel": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --rootDir . --testNamePattern=",
    "test-sequential": "yarn test-parallel --runInBand --detectOpenHandles",
    "test": "yarn test-parallel",
    "cypress-install": "cypress install --force",
    "cypress": "cypress open",
    "cypress-watch": "concurrently \"yarn:watch\" \"yarn:cypress\"",
    "cypress-run": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 cypress run --headed",
    "frontend-serve": "cross-env PORT=33922 NODE_ENV=production serve cra_output",
    "test-watch": "yarn test --watchAll",
    "test-build-watch": "concurrently \"yarn:watch:frontend-build\" \"yarn:test-watch\"",
    "postinstall": "patch-package",

    "electron-watch": "electronmon .",
    "preelectron-build-base": "cp src/electron.js cra_output/electron.js",
    "electron-build-base": "electron-builder --publish=onTag",
    "electron-build": "yarn electron-build-base --mac --win --linux --x64",

    "cordova-requirements": "cordova requirements",
    "emulate-android": "cordova emulate android",
    "build-android": "yarn build && rm -rf www && cp -r cra_output www && cordova build android --prod --release -- --keystore ~/.android/debug.keystore",
    "deploy-android": "adb install -d -r ./platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk",
    "run-android": "cordova run android --device"
  },
  "dependencies": {
    "@fontsource/roboto-condensed": "~4.4",

    "react": "~18.0.0-alpha-7ec4c5597",
    "react-dom": "~18.0.0-alpha-7ec4c5597",

    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",

    "recoil": "facebookexperimental/Recoil.git#nightly",
    "recoilize": "~2.0",

    "gun": "amark/gun.git#manhattan",

    "@chakra-ui/react": "~1.6",
    "@chakra-ui/system": "~1.6",
    "@chakra-ui/icons": "~1.0",
    "@chakra-ui/theme-tools": "~1.1",
    "@chakra-ui/media-query": "~1.0",
    "@emotion/react": "~11.4",
    "@emotion/styled": "~11.3",
    "framer-motion": "~4.1",
    "react-icons": "~4.2",

    "electron-is-dev": "~2.0",
    "electron-window-state": "~5.0",
    "electron-devtools-installer": "~3.2",
    "devtron": "~1.4",
    "https-localhost": "~4.6",

    "react-checkbox-tree": "~1.7",

    "react-markdown": "~6.0",
    "re-resizable": "~6.9",
    "date-fns": "~2.22",
    "jssha": "~3.2"
  },
  "devDependencies": {
    "@types/react": "~17.0",
    "@types/gun": "~0.9",

    "typescript": "~4.3",
    "ts-node": "~10.0",
    "@types/node" : "~15.12",
    "ts2fable" : "~0.8.0-build.615",

    "fable-loader": "~2.1",
    "fable-compiler": "~2.13",

    "electron": "~14.0.0-beta.6",
    "electron-builder": "~22.11",
    "electronmon": "~2.0",

    "@typescript-eslint/eslint-plugin": "~4.27",
    "@typescript-eslint/parser": "~4.27",
    "eslint": "~7.28",
    "eslint-plugin-react": "~7.24",
    "eslint-plugin-import": "~2.23",
    "eslint-plugin-jsx-a11y": "~6.4",
    "eslint-plugin-react-hooks": "~4.2",
    "eslint-plugin-jest-dom": "~3.9",

    "gatsby": "~3.7",
    "@chakra-ui/gatsby-plugin": "~2.0",
    "postcss": "~8.3",

    "jest": "~27.0",
    "jest-environment-jsdom": "~27.0",
    "ts-jest": "~27.0",
    "jest-css-modules": "~2.1",
    "@testing-library/react": "~11.2",
    "@testing-library/user-event": "~13.1",
    "@testing-library/jest-dom": "~5.14",

    "@babel/core": "~7.14",
    "@babel/preset-env": "~7.14",
    "@babel/preset-react": "~7.14",
    "@babel/preset-typescript": "~7.14",
    "babel-loader": "~8.2",
    "file-loader": "~6.2",
    "babel-jest": "~27.0",

    "cypress": "~7.5",
    "cypress-terminal-report": "~3.2",

    "patch-package": "~6.4",
    "postinstall-postinstall": "~2.1",

    "cross-env": "~7.0",
    "react-scripts": "~4.0",
    "react-app-rewired": "~2.1",
    "customize-cra": "~1.0",
    "customize-cra-react-refresh": "~1.1",
    "@craco/craco": "~6.1",
    "@dealmore/craco-plugin-babel-include": "~1.0",
    "cra-build-watch": "~4.0.0-beta.1",

    "parcel": "2.0.0-nightly.688",

    "react-refresh": "~0.10",

    "concurrently": "~6.2",

    "cordova": "~10.0",
    "cordova-android": "~10.0.0-nightly.2021.6.6.6d803e2f",
    "cordova-ios": "~6.2",
    "cordova-plugin-whitelist": "~1.3"
  },
  "build": {
    "appId": "fluke",
    "productName": "Fluke",
    "directories": {
      "output": "electron_output",
      "buildResources": "cra_output",
      "app": "."
    },
    "files": [
      "./cra_output/**/*"
    ],
    "extraMetadata": {
      "main": "cra_output/electron.js"
    },
    "win": {
      "icon": "./cra_output/icons/310x310.png",
      "target": [ "nsis", "zip" ]
    },
    "mac": {
      "icon": "./cra_output/icons/310x310.png",
      "category": "public.app-category.utilities",
      "target": [ "zip" ]
    },
    "linux": {
      "icon": "./cra_output/icons/310x310.png",
      "category": "Utility",
      "target": [ "AppImage", "snap" ]
    },
    "asar": "false",
    "nsis": {
      "oneClick": "false",
      "allowToChangeInstallationDirectory": "true",
      "installerIcon": "cra_output/favicon.ico",
      "installerHeaderIcon": "cra_output/favicon.ico",
      "deleteAppDataOnUninstall": false
    }
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "https://localhost:33921"
}
